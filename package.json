{
  "name": "@naverpay/device-info",
  "version": "1.0.29",
  "description": "Device information",
  "scripts": {
    "build": "tsup",
    "crawling": "node --experimental-modules ./src/crawling.mjs",
    "release": "changeset publish",
    "release:canary": "changeset publish --no-git-tag"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NaverPayDev/device-info.git"
  },
  "sideEffects": false,
  "files": [
    "./dist",
    "README.md",
    "README.ko.md",
    "CHANGELOG.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "author": "NaverPay Frontend team <https://github.com/NaverPayDev>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "node-html-parser": "^6.1.12",
    "playwright": "^1.56.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "packageManager": "pnpm@9.4.0"
}
